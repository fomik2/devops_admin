; name servers - NS records
    IN      NS      dnsmaster.mkdev.
    IN      NS      dnsclient.mkdev.

; name servers - A records
dnsmaster.mkdev.          IN      A       192.168.33.110

; 192.168.33.0/24 - A records
puppet.mkdev.       IN      A      192.168.33.1
nexus.mkdev.        IN      A      192.168.33.100
maven.mkdev.        IN      A      192.168.33.105
dnsclient.mkdev.    IN      A      192.168.33.111
