$TTL    604800
@       IN      SOA     {{ forward_zone_name  }}. admin.{{ forward_zone_name }}. (
                              4         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
